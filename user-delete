# useage ./user-delete "user_uuid" 

tenant=$(cat tenant)
okapi_url=$(cat okapi.url)
okapi_token=$(cat okapi.token)

user_uuid="${1}"

apicall=$(curl --http1.1 -s -w '\n' -X DELETE -H "Content-type: application/json" -H "X-Okapi-Tenant: ${tenant}" -H "x-okapi-token: ${okapi_token}" "${okapi_url}/users/${user_uuid}")

echo "${apicall}"
